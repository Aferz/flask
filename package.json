{
  "name": "flask-container",
  "version": "1.0.1",
  "author": "Alejandro Fernandez Rueda",
  "main": "./dist/flask.umd.js",
  "description": "IOC Container for Javascript & Node.js",
  "scripts": {
    "release": "sh build/release.sh",
    "pump:main": "node build/pumpVersion.js",
    "build:test": "webpack --config build/webpack.test.config.js",
    "build:dev": "webpack --config build/webpack.dev.config.js",
    "build:min": "webpack --config build/webpack.min.config.js",
    "build:umd": "webpack --config build/webpack.umd.config.js",
    "test": "mocha --compilers js:babel-core/register test/index.js",
    "test:coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- test/index.js",
    "karma": "npm run build:test && karma start ./build/karma.local.conf.js",
    "karma:sauce": "npm run build:test && karma start ./build/karma.sauce.conf.js"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "es6-symbol": "^3.1.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "string-replace-webpack-plugin": "0.0.4",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "toposort": "^1.0.0",
    "harmony-reflect": "^1.5.0"
  }
}
